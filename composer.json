{
	"name": "isochronlabs/elastic-apm-php-agent",
	"description": "A php 5.5 agent for Elastic APM",
    "license": "MIT",
    "require" : {
		"php": "5.5.*",
        "guzzlehttp/guzzle": "6.*",
        "ralouphie/getallheaders": "3.*",
        "ext-curl" : "*",
        "ext-json": "*"
    },
    "require-dev" : {
		"phpunit/phpunit": "*"
    },
    "suggest": {
        "ext-xdebug": "Required for processing of request headers"
    },
    "autoload" : {
        "psr-4" : {
            "PhilKra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhilKra\\Tests\\": "tests/"
        }
    },
    "config" : {
        "optimize-autoloader" : true
    },
    "authors" : [
        {
            "name" : "Philip Krauss",
            "email" : "philip@philipkrauss.at",
            "homepage" : "https://github.com/philkra"
        },
        {
            "name" : "George Boot",
            "email": "george@entryninja.com"
        },
        {
            "name" : "Mohamed Al Ashaal",
            "email" : "mohamed.alashaal@speakol.com",
            "homepage" : "https://github.com/speakol-ads"
        },
        {
            "name": "Joel Larson",
            "email": "isochronlabs@protonmail.com"
        }
    ]
}
